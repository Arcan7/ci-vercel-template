{
  "version": 2,
  "public": true,
  "builds": [
    {
      "src": "/api/index.php",
      "use": "vercel-php@0.7.0"
    },
    {
      "src": "/api/public/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/assets/(.*)",
      "headers": {
        "cache-control": "s-maxage=604800"
      },
      "dest": "/assets/$1"
    },
    {
      "src": "/(.*)",
      "dest": "api/index.php"
    }
  ]
}